{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "action": {
    "default_popup": "dist/popup.html",
    "default_title": "one tab"
  },
  "chrome_url_overrides": {
    "newtab": "dist/home.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
